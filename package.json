{
  "name": "ninepatch-vue",
  "version": "1.0.0",
  "description": "ninepatch-vue",
  "author": "midi_zeng <zengfukun@foxmail.com>",
  "maintainers": [
    "midi_zeng"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/types/index.d.ts",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/midi-zeng/ninepatch-vue.git"
  },
  "scripts": {
    "dev": "concurrently \"yarn docs:dev\" \"yarn dev:docs\"",
    "dev:docs": "export NODE_ENV=dev && rollup -w -c build/rollup.config.prod.js",
    "build": "export NODE_ENV=prod && rollup -c build/rollup.config.prod.js && yarn build:types",
    "lint": "eslint -c .eslintrc.js 'src/**/*.ts' --fix",
    "test": "export NODE_ENV=test && jest --color --coverage",
    "release:test": "semantic-release --ci false --debug",
    "release": "semantic-release",
    "build:types": "tsc --emitDeclarationOnly --declarationDir './dist/types'",
    "local": "yarn build && sudo yarn link"
  },
  "files": [
    "/dist/*"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "cross-env-shell commitlint -e $GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.6",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^12.1.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/gitlab": "^6.0.9",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "clean-css": "^5.1.2",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^10.2.7",
    "markdown-it-vuese": "^0.4.0",
    "node-sass": "^6.0.0",
    "postcss": "^8.2.15",
    "postcss-convert-unit": "^0.1.3",
    "postcss-loader": "^5.0.0",
    "postcss-px2rem": "^0.3.0",
    "postcss-url": "^8.0.0",
    "rollup": "^2.1.0",
    "rollup-plugin-asset-url": "^0.1.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "rollup-plugin-vue": "^5.1.6",
    "rollup-pluginutils": "^2.8.2",
    "sass-loader": "^11.0.1",
    "semantic-release": "^17.0.4",
    "ts-jest": "^26.1.3",
    "typescript": "^3.8.3",
    "vue": "^2.6.11",
    "vue-jest": "^3.0.5",
    "vue-property-decorator": "^8.4.1",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-vue": "^8.5.0",
    "ninepatch-core": "^1.0.3"
  },
  "peerDependencies": {
    "vue-property-decorator": "^8.4.1"
  }
}
